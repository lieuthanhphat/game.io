@charset "utf-8";
/* CSS Document */
*{
	padding: 0;
	margin: 0;
	box-sizing: border-box
}
body{
	background-color: #070A27
}
.nav-cl{
	background-color: #070A27;
	
	
}
.nav-item{
	margin-left: 10px;
	font-family: 'Ubuntu', sans-serif;
	font-size: 20px;
	
}
.nav-link{
	color: #E9E9E9!important;
	transition: all 0.5s
}
.nav-link:hover{
	color: #F50081!important
}
.d-block{
	width: 100%
}
.video{
	width: 100%
}
.carousel.slide{
	width: 400px;
	margin: 0 auto
}
.r1{
	width: 90%;
	height: 100px;
	background-color: #242E89;
	margin: 0 auto;
	margin-top: 50px;
	border-radius: 15px;
	-webkit-box-shadow: inset 0px 0px 69px -58px rgba(129,140,235,1);
	-moz-box-shadow: inset 0px 0px 69px -58px rgba(129,140,235,1);
	box-shadow: inset 0px 0px 69px -58px rgba(129,140,235,1);
	-webkit-box-shadow: 0px 0px 106px 3px rgba(0,1,3,1);
	-moz-box-shadow: 0px 0px 106px 3px rgba(0,1,3,1);
	box-shadow: 0px 0px 106px 3px rgba(0,1,3,1);
	transition: all 0.5s

}
.r1:hover{
	background-color: #3643B3
}
.dong1 img{
	width: 50px;
	margin-top: 25px;
	border-radius: 15px;
}
.dong1 h4{
	text-align: center;
	color: white
}
.dong1 p{
	color: #B5B5B5;
	text-align: center;
	margin-bottom: 0px
}
.dong1 h5{
	text-align: center;
	color: red;
	padding-top: 5px;
}
a{
	text-decoration: none;
}